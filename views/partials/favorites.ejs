<div class="ui favorites inverted overlay right vertical menu sidebar" style="z-index: 5;">
  <a class="header item">Favorites</a>
  <div class="item" ng-repeat="video in likes() | orderBy: 'title' track by $index">
    <i class="link {{videoInUpcoming(video.key) ? 'check' : 'plus'}} icon" ng-click="readd(video.key)"></i>
    <i class="link remove icon" ng-click="likeVideo(video)"></i>
    {{video.title}}
  </div>
</div>
